{
  "datacenter": "sz-dc",
  "data_dir": "/data/appdatas/consul",
  "log_level": "INFO",
  "enable_syslog": true,
  "node_name": "mynode",
  "server": true,
  "bootstrap_expect": 3,
  "bind_addr": "10.81.xxx.xxx1",
  "addresses": {
    "http": "0.0.0.0"
  },
  "ports": {
    "http": 8500
  },
  "ui": true,
  "performance": {
    "raft_multiplier": 1
  },
  "retry_join": [
    "10.81.xx.xxx2",
    "10.81.xx.xxx3"
  ],
  "retry_interval": "10s",
  "skip_leave_on_interrupt": true,
  "leave_on_terminate": false,
  "rejoin_after_leave": true
}
